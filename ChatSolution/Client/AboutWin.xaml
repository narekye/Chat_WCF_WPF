<Window x:Class="Client.AboutWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="AboutWin" Height="300" Width="420"
        ResizeMode="CanMinimize">
    <Grid>
        <StackPanel Margin="5" Orientation="Horizontal">
            <Button x:Name="NarGithub" Content="Narek GitHub" Margin="5,5,5,232"  Click="NarGithub_Click" Width="92" ></Button>
            <Button x:Name="VanGithub" Content="Van GitHub" Margin="5,5,5,232"  Click="VanGithub_Click" Width="83" ></Button>
            <Button x:Name="VanFB" Content="Van FaceBook" Margin="5,5,5,232"  Click="VanFB_Click" Width="98" ></Button>
            <Button x:Name="NarFB" Content="Narek FaceBook" Margin="5,5,5,232" Click="NarFB_Click" ></Button>

        </StackPanel>
    </Grid>
</Window>
